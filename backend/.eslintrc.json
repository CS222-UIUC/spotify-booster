{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "./node_modules/gts"
    ],
    "ignorePatterns": [
        "build/**/*"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
    ],
    "rules": {
        "node/no-unpublished-import": [
            "error",
            {
                "allowModules": [
                    "chai"
                ]
            }
        ]
    }
}